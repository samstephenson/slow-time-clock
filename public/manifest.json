{
  "manifest_version": 2,
  "short_name": "Slow Time",
  "name": "Slow Time new tab",
  "version": "1.0",
  "description": "24-hour clock face for your new tabs",
  "author": "Sam Stephenson",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "logo192.png"
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
